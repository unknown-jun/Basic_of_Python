        "a = [ 34, 22, 45, 27, 31, 33, 55 ]\n",
        "\n",
        "bond = '+'\n",
        "num = []\n",
        "\n",
        "for i in a:\n",
        "    num.append(str(i)) # 문자로 변경해서 b 리스트에 추가\n",
        "\n",
        "print(bond.join(num))"
